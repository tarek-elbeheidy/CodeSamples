<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <!-- Define some output appenders -->
  <appender name="MOEHESuspendedRequestsRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
    <file value="C:\\MOEHELogs\\SuspendedRequestslog.txt" />
    <appendToFile value="true" />
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <maxSizeRollBackups value="10" />
    <maximumFileSize value="10MB" />
    <rollingStyle value="Size" />
    <staticLogFileName value="true" />
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%d [%t] %-5p %c [%x] [%property{callerID}] [%property{Username}] - %m%n" />
    </layout>
  </appender>
  <logger name="MOEHESuspendedRequestsLogger">
    <level value="DEBUG" />
    <appender-ref ref="MOEHESuspendedRequestsRollingLogFileAppender" />
  </logger>
</log4net>
